{
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "./deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unclechu/test-01"
  },
  "author": "Viacheslav Lotsmanov <lotsmanov89@gmail.com>",
  "licenses": [
    {
      "type": "GNU/AGPLv3",
      "url": "https://raw.githubusercontent.com/unclechu/test-01/master/LICENSE"
    }
  ],
  "engines": {
    "iojs": ">=1.6.2"
  },
  "dependencies": {
    "LiveScript": "1.3.1",
    "bootstrap-styl": "^4.0.3",
    "bower": "^1.4.1",
    "co": "^4.5.1",
    "express": "^4.12.3",
    "express-promise": "~0.4.0",
    "front-end-gulp-pattern": "^1.0.1",
    "gulp-browserify": "~0.5.1",
    "gulp-stylus": "^2.0.1",
    "gulp-uglify": "^1.1.0",
    "jade": "^1.9.2",
    "js-yaml": "^3.2.7",
    "liveify-stable": "^1.0.0",
    "mz": "^1.3.0",
    "nib": "^1.1.0",
    "prelude-ls": "1.1.1",
    "winston": "^1.0.0"
  },
  "gulp": {
    "distclean": [
      "./node_modules",
      "./bower_components"
    ],
    "styles": {
      "main": {
        "type": "stylus",
        "path": "./front-end-sources/styles/",
        "srcDir": "./front-end-sources/styles/",
        "mainSrc": "main.styl",
        "destDir": "./static/",
        "buildFile": "build.css",
        "shim": [
          "./stylus-shim/nib",
          "./stylus-shim/bootstrap"
        ]
      }
    },
    "scripts": {
      "main": {
        "type": "browserify",
        "path": "./front-end-sources/scripts/",
        "srcDir": "./front-end-sources/scripts/",
        "mainSrc": "main.ls",
        "destDir": "./static/",
        "buildFile": "build.js",
        "transform": [
          "liveify-stable"
        ],
        "extensions": [
          ".ls"
        ],
        "jshintDisabled": true,
        "shim": {
          "highcharts": {
            "path": "./bower_components/highcharts/highcharts.js",
            "exports": "Highcharts"
          }
        }
      }
    }
  }
}
